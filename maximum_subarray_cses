int32_t main(){
   int n;
   cin>>n;
   int arr[n];
   
   for(int x=0; x<n; x++){
       cin>>arr[x];
   }
   
   int sum=arr[0];
   int max1=arr[0];
   
   for(int x=1; x<n; x++){
       sum=max(arr[x],sum+arr[x]);
       max1=max(max1,sum);
   }
   cout<<max1<<endl;
}
   
   
